{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/sass/*"],
      "@modules/*": ["src/modules/*"]
    },
    "jsx": "react",
    "esModuleInterop": true
  },

  "exclude": ["node_modules", "dist"]
}
